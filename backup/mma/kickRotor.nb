    RowBox[{"p", "\[LeftDoubleBracket]", 
     RowBox[{"x", ",", "p"}], "\[RightDoubleBracket]"}]}], 
   "*)"}]}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"phi2Ph", "[", "phi_", "]"}], ":=", 
   RowBox[{"Catch", "[", "\[IndentingNewLine]", 
    RowBox[{"Block", "[", "\[IndentingNewLine]", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"usdphi", ",", "res"}], "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"res", "=", 
        RowBox[{"Table", "[", "\[IndentingNewLine]", 
         RowBox[{
          RowBox[{
           RowBox[{"usdphi", "=", 
            RowBox[{"Table", "[", 
             RowBox[{
              RowBox[{"phi", "\[LeftDoubleBracket]", 
               RowBox[{
                RowBox[{"1", "+", 
                 RowBox[{
                  RowBox[{"(", 
                   RowBox[{
                    RowBox[{"j", "*", "m"}], "+", "p"}], ")"}], "*", "m"}]}], 
                ";;", 
                RowBox[{"m", "+", 
                 RowBox[{
                  RowBox[{"(", 
                   RowBox[{
                    RowBox[{"j", "*", "m"}], "+", "p"}], ")"}], "*", 
                  "m"}]}]}], "\[RightDoubleBracket]"}], ",", 
              RowBox[{"{", 
               RowBox[{"j", ",", "0", ",", 
                RowBox[{"per", "-", "1"}]}], "}"}]}], "]"}]}], ";", 
           "\[IndentingNewLine]", 
           RowBox[{"Total", "[", 
            RowBox[{
             RowBox[{
              RowBox[{
               RowBox[{"Abs", "[", 
                RowBox[{"Fourier", "[", "#", "]"}], "]"}], "^", "2"}], "&"}], 
             "/@", "usdphi"}], "]"}]}], ",", "\[IndentingNewLine]", 
          RowBox[{"{", 
           RowBox[{"p", ",", "0", ",", 
            RowBox[{"m", "-", "1"}]}], "}"}]}], "\[IndentingNewLine]", 
         "]"}]}], ";", "\[IndentingNewLine]", 
       RowBox[{"Throw", "[", "res", "]"}]}]}], "\[IndentingNewLine]", "]"}], 
    "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"xpGrid", "=", 
   RowBox[{"Flatten", "[", 
