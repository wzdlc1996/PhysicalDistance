Cell[BoxData[{
 RowBox[{
  RowBox[{
   RowBox[{"gaus", "[", 
    RowBox[{"x_", ",", 
     RowBox[{"{", 
      RowBox[{"x0_", ",", "p0_"}], "}"}]}], "]"}], ":=", 
   RowBox[{"Exp", "[", 
    RowBox[{
     FractionBox[
      RowBox[{"-", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"x", "-", "x0"}], ")"}], "^", "2"}]}], 
      RowBox[{"4.", "*", 
       RowBox[{
        RowBox[{"(", 
         RowBox[{"2.", 
          RowBox[{"\[Pi]", "/", "m"}]}], ")"}], "^", "2"}]}]], "+", 
     FractionBox[
      RowBox[{"I", 
       RowBox[{"(", 
        RowBox[{"x", "-", "x0"}], ")"}], "*", "p0", " "}], "hbar"]}], "]"}]}],
   ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"qSpec", "=", 
   RowBox[{"Array", "[", 
    RowBox[{
     RowBox[{
      RowBox[{
       FractionBox[
        RowBox[{"2.", "\[Pi]"}], "dim"], 
       RowBox[{"(", 
        RowBox[{"#", "-", "1."}], ")"}]}], "&"}], ",", "dim"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"pSpec", "=", 
   RowBox[{
    RowBox[{"Array", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"#", "-", "1."}], "&"}], ",", "dim"}], "]"}], "*", "hbar"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"qPhase", "=", 
   RowBox[{
    RowBox[{
     RowBox[{"Exp", "[", 
      RowBox[{
       RowBox[{"-", "I"}], " ", "K", " ", 
       RowBox[{
        RowBox[{"Cos", "[", "#", "]"}], "/", "hbar"}]}], "]"}], "&"}], "/@", 
    "qSpec"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"pPhase", "=", 
   RowBox[{
    RowBox[{
     RowBox[{"Exp", "[", 
      RowBox[{
       RowBox[{"-", "I"}], " ", 
       RowBox[{
        RowBox[{"#", "^", "2"}], "/", 
        RowBox[{"(", 
         RowBox[{"2.", "*", "hbar"}], ")"}]}]}], "]"}], "&"}], "/@", 
    "pSpec"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"q2p", "[", "lis_", "]"}], ":=", 
   RowBox[{"InverseFourier", "[", "lis", "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"p2q", "[", "lis_", "]"}], ":=", 
   RowBox[{"Fourier", "[", "lis", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"phiLis", "=", 
   RowBox[{"ParallelTable", "[", 
    RowBox[{
     RowBox[{
      RowBox[{
       FractionBox["#", 
        RowBox[{"Norm", "[", "#", "]"}]], "&"}], "[", 
      RowBox[{"q2p", "[", 
       RowBox[{
        RowBox[{
         RowBox[{"gaus", "[", 
          RowBox[{"#", ",", "obj"}], "]"}], "&"}], "/@", "qSpec"}], "]"}], 
      "]"}], ",", 
     RowBox[{"{", 
      RowBox[{"obj", ",", "objLis"}], "}"}]}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"evo", "[", "phi_", "]"}], ":=", 
   RowBox[{
    RowBox[{"q2p", "[", 
     RowBox[{"qPhase", "*", 
      RowBox[{"p2q", "[", "phi", "]"}]}], "]"}], "*", "pPhase"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"evoPath", "[", "ini_", "]"}], ":=", 
   RowBox[{"Catch", "[", "\[IndentingNewLine]", 
    RowBox[{"Block", "[", 
     RowBox[{
      RowBox[{"{", 
       RowBox[{"path", "=", 
        RowBox[{"{", "ini", "}"}]}], "}"}], ",", "\[IndentingNewLine]", 
      RowBox[{
       RowBox[{"Do", "[", "\[IndentingNewLine]", 
        RowBox[{
         RowBox[{
          RowBox[{"AppendTo", "[", 
           RowBox[{"path", ",", 
            RowBox[{"evo", "[", 
             RowBox[{"path", "[", 
              RowBox[{"[", 
               RowBox[{"-", "1"}], "]"}], "]"}], "]"}]}], "]"}], ";"}], 
         "\[IndentingNewLine]", ",", 
         RowBox[{"{", "trajLen", "}"}]}], "\[IndentingNewLine]", "]"}], ";", 
       "\[IndentingNewLine]", 
       RowBox[{"Throw", "[", "path", "]"}]}]}], "\[IndentingNewLine]", "]"}], 
    "\[IndentingNewLine]", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"SetSharedVariable", "[", "phiTLis", "]"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"phiTLis", "=", 
    RowBox[{"ParallelMap", "[", 
     RowBox[{
      RowBox[{
       RowBox[{"evoPath", "[", "#", "]"}], "&"}], ",", "phiLis"}], "]"}]}], 
   ";"}], "\[IndentingNewLine]", 
  RowBox[{"(*", 
   RowBox[{"Phase", " ", "Space", " ", "Representation"}], "*)"}], 
  "\[IndentingNewLine]", 
  RowBox[{"(*", 
