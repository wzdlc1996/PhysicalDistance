          RowBox[{"\[Pi]", "/", "m"}]}]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"p", ",", "0", ",", 
         RowBox[{"m", "-", "1"}]}], "}"}], ",", 
       RowBox[{"{", 
        RowBox[{"x", ",", "0", ",", 
         RowBox[{"m", "-", "1"}]}], "}"}]}], "]"}], ",", "1"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{
   RowBox[{"dist", "[", 
    RowBox[{"x_", ",", "y_"}], "]"}], ":=", 
